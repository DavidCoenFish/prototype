	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "package production",
			"runtimeExecutable": "npm",
			"windows": {
				"runtimeExecutable": "npm.cmd"
			},
			"runtimeArgs": [
				"run-script",
				"packagevsc"
			],
			"env": {
				"NODE_ENV": "production"
			},
			"port": 5858,
			"timeout": 100000
			//"console": "externalTerminal"
		},
		{
			"type": "node",
			"request": "launch",
			"name": "package development",
			"runtimeExecutable": "npm",
			"windows": {
				"runtimeExecutable": "npm.cmd"
			},
			"runtimeArgs": [
				"run-script",
				"packagevsc"
			],
			"env": {
				"NODE_ENV": "development"
			},
			"port": 5858,
			"timeout": 100000,
		}
	]
}